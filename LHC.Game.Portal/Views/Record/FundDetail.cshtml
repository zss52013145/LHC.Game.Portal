@using ShenZhen.Lottery.Model



<div data-v-3b9d91bc="" data-v-d6475828="" class="dialog_wrap" style="">
    <div data-v-3b9d91bc="" class="dialog" style="min-height: 500px;" flag="true">
        <div data-v-3b9d91bc="" class="t_b-c dialog_title" style="cursor: move;">
            资金明细
      <a data-v-3b9d91bc="" href="javascript:;" class="dialog_close"></a>
        </div>
        <div data-v-3b9d91bc="" class="dialog_content" style="min-height: 500px;">
            <div data-v-c187c242="" data-v-d6475828="" class="table-wrap" data-v-3b9d91bc="">
                <table data-v-c187c242="" class="table">
                    <tr data-v-c187c242="">
                        <th data-v-c187c242="">余额</th>
                        <th data-v-c187c242="">变动金额</th>
                        <th data-v-c187c242="">类型</th>
                        <th data-v-c187c242="">备注</th>
                        <th data-v-c187c242="">时间</th>
                    </tr>

                    @foreach (ProfitLoss p in ViewBag.list)
                    {
                        <tr data-v-c187c242="">
                            <td data-v-c187c242="">@p.CurrentMoney</td>
                            <td data-v-c187c242="">

                                @if (p.Money > 0)
                                {
                                    <span data-v-d6475828="" data-v-c187c242="" style="color: red;">@p.Money</span>
                                }
                                else
                                {
                                    <span data-v-d6475828="" data-v-c187c242="" style="color: green;">@p.Money</span>
                                }
                            </td>
                            <td data-v-c187c242="">
                                <span data-v-d6475828="" data-v-c187c242="">@p.ShowOp</span>
                            </td>
                            <td data-v-c187c242="">@p.Mark</td>
                            <td data-v-c187c242="">
                                <span data-v-d6475828="" data-v-c187c242="">@p.ShowTime</span>
                            </td>
                        </tr>
                    }






                    <!---->
                </table>
            </div>
        </div>
    </div>
</div>
