@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>前台</title>
    <link href="/static/css/app.bdf2bc5bf14cea19c93045c00598b783.css" rel="stylesheet">
    <style type="text/css">
        .message[data-v-0da2e404] {
            position: absolute;
            display: flex;
            top: 0.4rem;
            padding: 0.32rem;
            height: auto;
            justify-content: center;
            align-items: center;
            border-radius: 0.13333rem;
            font-size: 0.37333rem;
            z-index: 99999;
            box-shadow: 0px 0px 0.2rem #ccc;
            background: #fff;
        }

        .message__icon[data-v-0da2e404] {
            width: 0.8rem;
            height: 0.8rem;
        }

        .message__icon--info[data-v-0da2e404] {
            background: url(data:image/png;base64,) no-repeat;
            background-size: contain;
        }

        .message__icon--error[data-v-0da2e404] {
            width: 0.69333rem;
            height: 0.69333rem;
            background: url(data:image/png;base64,) no-repeat;
            background-size: contain;
        }

        .message__icon--warning[data-v-0da2e404] {
            background: url(data:image/png;base64,) no-repeat;
            background-size: contain;
        }

        .message__icon--success[data-v-0da2e404] {
            background: url(data:image/png;base64,) no-repeat;
            background-size: contain;
        }

        .message__content[data-v-0da2e404] {
            margin-left: 0.13333rem;
            line-height: 0.8rem;
            padding: 0.13333rem;
        }

        .slide-fade-enter-active[data-v-0da2e404] {
            transition: all .3s ease;
        }

        .slide-fade-leave-active[data-v-0da2e404] {
            transition: all 0.6s cubic-bezier(1, 0.5, 0.8, 1);
        }

        .slide-fade-enter[data-v-0da2e404], .slide-fade-leave-to[data-v-0da2e404] {
            transform: translateY(-0.8rem);
            opacity: 0;
        }
    </style>
    <style type="text/css">
        .notification[data-v-08b46fc5] {
            width: 100%;
            height: 0;
            display: flex;
            justify-content: center;
        }
    </style>
</head>
<body>
    <div class="app_wrap">
        <div data-v-0537b444="" data-v-7e642308="" class="login clearBoth">
            <div data-v-0537b444="" class="login-form">
                @using (Ajax.BeginForm("Login", "Home", new AjaxOptions() { OnBegin = "beforeLogin", OnSuccess = "afterLogin" }))
                {
                    <p data-v-0537b444="" class="noMargin">用户名</p>
                    <label data-v-0537b444="">
                        <input data-v-0537b444="" type="text" placeholder="用户名" name="username">
                    </label>
                    <p data-v-0537b444="">密码</p>
                    <label data-v-0537b444="">
                        <input data-v-0537b444="" type="password" placeholder="密码" name="password">
                    </label>
@*  <p data-v-0537b444="">验证码</p>
                    <div data-v-0537b444="" class="code-wrap">
                        <label data-v-0537b444="">
                            <input data-v-0537b444="" type="text" placeholder="验证码">
                        </label>
                        <img data-v-0537b444="" src="/api/ssc_golang/v1/user/captcha?n=2&amp;random=1649036631581" class="code">
                    </div>*@
                    <div data-v-0537b444="">
                        <button data-v-0537b444="" class="normal-btn submit" type="submit">登录</button>
                        @*<button data-v-0537b444="" class="normal-btn submit2">手机版</button>*@
                    </div>
                }
            </div>
        </div>
    </div>

    @*<div id="messageBox"></div>*@

    <script src="/js/jquery-1.8.3.min.js"></script>
    <script src="/js/jquery.unobtrusive-ajax.min.js"></script>
    <script src="/js/beforeAndAfter.js"></script>

    <script src="/js/function.js"></script>


    @RenderPage("../Public/Prompt.cshtml")

</body>
</html>
